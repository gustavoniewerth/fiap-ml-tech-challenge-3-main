       "<Figure size 1000x800 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Definir colunas estratégicas\n",
    "cols_potential = [\n",
    "    'DEPARTURE_DELAY', 'ARRIVAL_DELAY',  # Atrasos\n",
    "    'TAXI_OUT', 'TAXI_IN',               # Congestionamento em solo\n",
    "    'AIR_TIME', 'DISTANCE',              # Características físicas\n",
    "    'SCHEDULED_TIME', 'ELAPSED_TIME'     # Planejamento vs Realidade\n",
    "]\n",
    "\n",
    "# Filtrar apenas as que existem no seu dataframe (segurança contra erro de digitação)\n",
    "cols_corr = [col for col in cols_potential if col in flights_enriched_clean.columns]\n",
    "\n",
    "# Calcular Correlação\n",
    "correlation = flights_enriched_clean[cols_corr].corr()\n",
    "\n",
    "# Plotar\n",
    "plt.figure(figsize=(10, 8))\n",
    "sns.heatmap(correlation, annot=True, cmap='coolwarm', fmt=\".2f\", vmin=-1, vmax=1)\n",
    "plt.title(\"Matriz de Correlação: Operação e Atrasos\")\n",
    "plt.show()"
   ]
  },
