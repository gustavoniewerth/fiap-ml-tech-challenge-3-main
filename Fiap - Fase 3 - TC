   "source": [
    "# Definindo as colunas numéricas/categóricas base (Features)\n",
    "cols_base = [\n",
    "    'MONTH', 'DAY', 'DAY_OF_WEEK', \n",
    "    'ORIGIN_AIRPORT', 'DESTINATION_AIRPORT', \n",
    "    'SCHEDULED_DEPARTURE', 'SCHEDULED_ARRIVAL', \n",
    "    'SCHEDULED_TIME', 'DISTANCE', \n",
    "    'ROUTE', 'DEP_HOUR'\n",
    "]\n",
    "\n",
    "# Definindo o Alvo (Target)\n",
    "col_target = ['IS_DELAYED']\n",
    "\n",
    "# Pegando dinamicamente as colunas do One-Hot Encoding\n",
    "# (Todas que começam com AIRLINE_ ou PERIOD_OF_DAY_)\n",
    "cols_dummies = [col for col in flights_encoded.columns \n",
    "                if col.startswith('AIRLINE_') or col.startswith('PERIOD_OF_DAY_')]\n",
    "\n",
    "# Juntando tudo\n",
    "final_columns = cols_base + cols_dummies + col_target\n",
    "\n",
    "# Criando o DataFrame Final\n",
    "flights_final = flights_encoded[final_columns].copy()\n",
    "\n",
    "# --- Verificação ---\n",
    "print(f\"Dataset pronto para modelagem!\")\n",
    "print(f\"Linhas: {flights_final.shape[0]}\")\n",
    "print(f\"Colunas: {flights_final.shape[1]}\")"
   ]
  },
