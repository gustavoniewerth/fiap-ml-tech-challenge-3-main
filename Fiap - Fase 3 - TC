    "weekly_delay = flights_enriched_clean.groupby('DAY_OF_WEEK')['IS_DELAYED'].mean().reset_index()\n",
    "weekly_delay['DAY_OF_WEEK'] = weekly_delay['DAY_OF_WEEK'].astype(int)\n",
    "day_map = {1: 'Seg', 2: 'Ter', 3: 'Qua', 4: 'Qui', 5: 'Sex', 6: 'Sáb', 7: 'Dom'}\n",
    "weekly_delay['DAY_NAME'] = weekly_delay['DAY_OF_WEEK'].map(day_map)\n",
    "\n",
    "plt.figure(figsize=(12, 6))\n",
    "\n",
    "# Plotando\n",
    "ax = sns.barplot(\n",
    "    x='DAY_NAME', \n",
    "    y='IS_DELAYED', \n",
    "    data=weekly_delay.sort_values('DAY_OF_WEEK'), \n",
    "    color=\"skyblue\"\n",
    ")\n",
