    "# --- ETAPA 1: JOIN COM AIRLINES ---\n",
    "\n",
    "# O objetivo é trazer o nome da companhia.\n",
    "# Renomeamos a coluna no dataset auxiliar para evitar colisão de nomes\n",
    "airlines = airlines.rename(columns={'AIRLINE': 'AIRLINE_NAME'})\n",
    "\n",
    "flights_enriched = flights.merge(\n",
    "    airlines[['IATA_CODE', 'AIRLINE_NAME']],\n",
    "    left_on='AIRLINE',\n",
    "    right_on='IATA_CODE',\n",
    "    how='left'\n",
    ")\n",
    "\n",
    "# Removemos a coluna duplicada da chave (IATA_CODE) para limpar\n",
    "flights_enriched = flights_enriched.drop(columns=['IATA_CODE'])"
   ]
  },
  {
   "cell_type": "code",
