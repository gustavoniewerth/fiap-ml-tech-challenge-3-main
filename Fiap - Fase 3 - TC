    "# ==============================================================================\n",
    "# 1. PREPARAÇÃO DOS DADOS PARA GPU\n",
    "# ==============================================================================\n",
    "\n",
    "# Criamos uma cópia do dataset final para este cenário\n",
    "df_scenario1 = flights_final.copy()\n",
    "df_scenario1 = df_scenario1.select_dtypes(exclude=['object'])\n",
    "\n",
    "# Converter tudo para float32 - GPUs NVIDIA são otimizadas para float32).\n",
    "for col in df_scenario1.columns:\n",
    "    df_scenario1[col] = df_scenario1[col].astype('float32')\n",
    "\n",
    "# Separar Features (X) e Target (y)\n",
    "X = df_scenario1.drop(columns=['IS_DELAYED'])\n",
    "y = df_scenario1['IS_DELAYED']"
   ]
  },
  {
   "cell_type": "code",
