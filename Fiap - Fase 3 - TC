    "# Label Encoding para Aeroportos e Rotas (Alta cardinalidade)\n",
    "label_cols = ['ORIGIN_AIRPORT', 'DESTINATION_AIRPORT', 'ROUTE']\n",
    "le_dict = {} # Dicionário para guardar os codificadores se precisarmos inverter depois\n",
    "\n",
    "for col in label_cols:\n",
    "    le = LabelEncoder()\n",
    "    # Convertendo para string antes para garantir que não haja erro de tipo misto\n",
    "    flights_encoded[col] = le.fit_transform(flights_encoded[col].astype(str))\n",
    "    le_dict[col] = le\n",
    "\n",
    "print(\"Feature Engineering Finalizado!\")"
   ]
