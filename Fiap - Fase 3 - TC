   "source": [
    "# 1. Agrupando e Filtrando \n",
    "airport_stats = flights_enriched_clean.groupby('ORIGIN_AIRPORT')['IS_DELAYED'].agg(['mean', 'count']).reset_index()\n",
    "min_flights = 50 \n",
    "airport_stats_filtered = airport_stats[airport_stats['count'] >= min_flights]\n",
    "\n",
    "# Top 15 e Ordenação\n",
    "top15_airports = airport_stats_filtered.sort_values(by='mean', ascending=False).head(15)\n",
    "\n",
    "# --- Plotagem ---\n",
    "plt.figure(figsize=(15, 8)) # Aumentei um pouco a altura\n",
    "\n",
    "ax = sns.barplot(\n",
    "    x='ORIGIN_AIRPORT', \n",
    "    y='mean', \n",
    "    data=top15_airports, \n",
    "    palette=\"Reds_r\",\n",
    "    order=top15_airports['ORIGIN_AIRPORT'] \n",
    ")\n",
    "\n",
    "plt.title(f\"Top 15 Aeroportos com Maior % de Atrasos (Mínimo {min_flights} voos)\", fontsize=14)\n",
    "plt.xlabel(\"Aeroporto de Origem\")\n",
    "plt.ylabel(\"Porcentagem de Voos Atrasados\")\n",
    "\n",
    "# Linha da Média Global\n",
    "global_mean = flights_enriched_clean['IS_DELAYED'].mean()\n",
    "plt.axhline(global_mean, color='red', linestyle='--', label=f'Média Global ({global_mean*100:.1f}%)')\n",
    "plt.legend()\n",
    "\n",
    "ax.yaxis.set_major_formatter(mtick.PercentFormatter(1.0))\n",
    "\n",
    "# --- ADICIONANDO O NÚMERO DE VOOS (N) ---\n",
    "for i, p in enumerate(ax.patches):\n",
    "    height = p.get_height()\n",
    "    count_val = top15_airports.iloc[i]['count']\n",
    "    \n",
    "    ax.text(\n",
    "        x = p.get_x() + p.get_width()/2,  \n",
    "        y = height,                       \n",
    "        s = f\"N={int(count_val)}\",       \n",
    "        ha = 'center', \n",
    "        va = 'bottom', \n",
    "        fontsize = 10, \n",
    "        color = 'black',\n",
    "        fontweight = 'bold',\n",
    "        rotation = 45 \n",
    "    )\n",
    "\n",
    "plt.ylim(0, top15_airports['mean'].max() * 1.15) \n",
    "plt.show()"
   ]
