   "source": [
    "# Plotando Gráfico ROC\n",
    "\n",
    "# 1. Calculando a métrica AUC (Area Under Curve)\n",
    "auc_score = roc_auc_score(y_test, y_proba)\n",
    "\n",
    "# 2. Calculando os pontos da curva (FPR, TPR)\n",
    "fpr, tpr, thresholds = roc_curve(y_test, y_proba)\n",
    "\n",
    "# --- PLOT DA CURVA ROC ---\n",
    "plt.figure(figsize=(8, 6))\n",
    "\n",
    "# Plot da curva do modelo\n",
    "plt.plot(fpr, tpr, color='darkorange', lw=2, label=f'Curva ROC (AUC = {auc_score:.4f})')\n",
    "\n",
    "# Plot da linha de referência (aleatória)\n",
