   "source": [
    "# Garantimos que as colunas sejam tratadas como texto para a verificação\n",
    "flights_enriched['ORIGIN_AIRPORT'] = flights_enriched['ORIGIN_AIRPORT'].astype(str)\n",
    "flights_enriched['DESTINATION_AIRPORT'] = flights_enriched['DESTINATION_AIRPORT'].astype(str)\n",
    "\n",
    "# Criamos filtros para identificar onde o valor é numérico (dígitos)\n",
    "mask_origem_num = flights_enriched['ORIGIN_AIRPORT'].str.isnumeric()\n",
    "mask_destino_num = flights_enriched['DESTINATION_AIRPORT'].str.isnumeric()\n",
    "\n",
    "# Contagem\n",
    "qtd_origem_num = mask_origem_num.sum()\n",
    "qtd_destino_num = mask_destino_num.sum()\n",
    "\n",
    "print(f\"Total de voos com ORIGEM numérica (DOT ID): {qtd_origem_num}\")\n",
    "print(f\"Total de voos com DESTINO numérico (DOT ID): {qtd_destino_num}\")\n",
    "\n",
    "# Verificando a porcentagem em relação ao total\n",
    "total_linhas = len(flights_enriched)\n",
    "print(f\"\\nPorcentagem de origens numéricas: {(qtd_origem_num / total_linhas) * 100:.2f}%\")"
   ]
