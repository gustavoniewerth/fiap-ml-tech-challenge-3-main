    "# ==============================================================================\n",
    "# 1. CÁLCULO DO PESO (SCALE_POS_WEIGHT)\n",
    "# ==============================================================================\n",
    "\n",
    "num_negativos = y_train.value_counts()[0] # Pontuais\n",
    "num_positivos = y_train.value_counts()[1] # Atrasados\n",
    "weight = num_negativos / num_positivos\n",
    "\n",
    "print(f\"Fator de Balanceamento calculado: {weight:.2f}\")\n",
    "print(\"O modelo dará esse peso extra para cada erro na classe de Atraso.\")"
   ]
