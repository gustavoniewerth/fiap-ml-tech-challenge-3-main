   "source": [
    "# ==============================================================================\n",
    "# 2. CONFIGURAÇÃO E TREINO (XGBOOST GPU)\n",
    "# ==============================================================================\n",
    "# Instanciando o classificador\n",
    "model_xgb = xgb.XGBClassifier(\n",
    "    n_estimators=100,        # Mesmo número do RF\n",
    "    max_depth=20,            # XGBoost geralmente precisa de menos profundidade que RF\n",
    "    learning_rate=0.05,      # Taxa de aprendizado (mais baixo = mais preciso, mas demora mais)\n",
    "    \n",
    "    # --- Configurações de GPU ---\n",
    "    tree_method='hist',      # Método de histograma (muito rápido)\n",
    "    device='cuda',           # Força o uso da GPU (em versões antigas use tree_method='gpu_hist')\n",
    "    \n",
    "    # --- O Pulo do Gato para o Recall ---\n",
    "    scale_pos_weight=weight, # Substitui o undersampling manual!\n",
    "    \n",
    "    # Outros\n",
    "    random_state=42,\n",
    "    objective='binary:logistic',\n",
    "    eval_metric='auc'\n",
    ")\n",
    "\n",
    "# Treinando com o X_train ORIGINAL (com todas as linhas)\n",
    "model_xgb.fit(X_train, y_train)\n",
    "print(\"Modelo XGBoost treinado!\")"
   ]
