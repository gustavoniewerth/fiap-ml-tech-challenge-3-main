    "# --- ETAPA 2: JOIN COM AIRPORTS (ORIGEM) ---\n",
    "\n",
    "# Vamos preparar o dataset de aeroportos para a ORIGEM\n",
    "airports_origin = airports[['IATA_CODE', 'AIRPORT', 'COUNTRY']].copy()\n",
    "airports_origin.columns = ['IATA_CODE_ORIGIN', 'ORIGIN_AIRPORT_NAME', 'ORIGIN_COUNTRY']\n",
    "\n",
    "flights_enriched = flights_enriched.merge(\n",
    "    airports_origin,\n",
    "    left_on='ORIGIN_AIRPORT',\n",
    "    right_on='IATA_CODE_ORIGIN',\n",
    "    how='left'\n",
    ")\n",
    "\n",
    "# Removemos a chave duplicada\n",
    "flights_enriched = flights_enriched.drop(columns=['IATA_CODE_ORIGIN'])"
   ]
